  package com.company;

public class Fourth_day {
    public static void main(String[] args) {
  //Problem 3 -- Reverse of a given Number
    int number = 987654, reverse = 0;
    while (number!=0){
        int remainder = number%10;
        reverse = reverse * 10+ remainder;
        number = number/10;
    }
    }
    }
